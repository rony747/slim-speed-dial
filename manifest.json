{
  "manifest_version": 3,
  "name": "Slim Speed Dial",
  "version": "1.1.0",
  "description": "Slim speed dial with group management",
  "permissions": [
    "storage",
    "tabs",
    "bookmarks",
    "activeTab",
    "windows",
    "contextMenus",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "js/background.js"
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
